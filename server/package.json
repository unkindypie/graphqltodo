{
  "name": "todoapp",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migration:generate": "cd ./src && ts-node-dev ../node_modules/typeorm/cli.js migration:generate -n",
    "migration:create": "cd ./src && ts-node-dev ../node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "cd ./src && ts-node-dev ../node_modules/typeorm/cli.js migration:run",
    "db:setup": "ts-node ./src/test-utils/setup.ts",
    "start": "node dist/index.js",
    "test": "npm run db:setup && jest",
    "dev": "ts-node-dev  ./src/index.ts",
    "watch": "tsc -w",
    "devjs": "nodemon dist/index.js",
    "clear_port": "fuser -k 4000/tcp && echo 'Terminated' || echo 'Nothing was running on the PORT'",
    "compile": "tsc -p ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/connect-redis": "*",
    "@types/cors": "*",
    "@types/express": "^4.17.8",
    "@types/express-session": "*",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.6",
    "@types/redis": "*",
    "gts": "^3.0.2",
    "jest": "^27.0.4",
    "nodemon": "^2.0.6",
    "ts-jest": "^27.0.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "*",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "apollo-server-express": "^2.18.2",
    "argon2": "^0.27.0",
    "class-validator": "*",
    "connect-redis": "*",
    "cors": "*",
    "dotenv": "*",
    "express": "^4.17.1",
    "express-session": "*",
    "graphql": "^15.4.0",
    "pg": "^8.6.0",
    "redis": "*",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.33",
    "typeorm-typedi-extensions": "^0.4.1"
  }
}
