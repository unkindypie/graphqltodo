{
  "name": "todoapp",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "devjs": "nodemon dist/index.js",
    "_dev": "nodemon  --watch './src/**/*.ts' --exec 'ts-node -P tsconfig.json' ./src/index.ts",
    "dev": "ts-node-dev  ./src/index.ts",
    "clear_port": "fuser -k 4000/tcp && echo 'Terminated' || echo 'Nothing was running on the PORT'",
    "create:migration": "mikro-orm migration:create",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@mikro-orm/cli": "^4.2.3",
    "@types/connect-redis": "*",
    "@types/cors": "*",
    "@types/express": "^4.17.8",
    "@types/express-session": "*",
    "@types/node": "^14.14.6",
    "@types/redis": "*",
    "gts": "^3.0.2",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "ts-node-dev": "*",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@mikro-orm/core": "^4.2.3",
    "@mikro-orm/migrations": "^4.2.3",
    "@mikro-orm/postgresql": "^4.2.3",
    "apollo-server-express": "^2.18.2",
    "argon2": "^0.27.0",
    "connect-redis": "*",
    "cors": "*",
    "dotenv": "*",
    "express": "^4.17.1",
    "express-session": "*",
    "graphql": "^15.4.0",
    "pg": "^8.4.2",
    "redis": "*",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.0"
  }
}
