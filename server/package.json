{
  "name": "todoapp",
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "devjs": "nodemon dist/index.js",
    "_dev": "nodemon  --watch './src/**/*.ts' --exec 'ts-node -P tsconfig.json' ./src/index.ts",
    "dev": "ts-node-dev  ./src/index.ts",
    "clear_port": "fuser -k 4000/tcp && echo 'Terminated' || echo 'Nothing was running on the PORT'",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:generate": "cd ./src && ts-node-dev ../node_modules/typeorm/cli.js migration:generate -n",
    "migration:create": "cd ./src && ts-node-dev ../node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "typeorm migration:run",
    "lint": "gts lint",
    "_clean": "gts clean",
    "compile": "tsc -p .",
    "_fix": "gts fix",
    "_prepare": "npm run compile",
    "_pretest": "npm run compile",
    "_posttest": "npm run lint"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/connect-redis": "*",
    "@types/cors": "*",
    "@types/express": "^4.17.8",
    "@types/express-session": "*",
    "@types/node": "^14.14.6",
    "@types/redis": "*",
    "gts": "^3.0.2",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "ts-node-dev": "*",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "apollo-server-express": "^2.18.2",
    "argon2": "^0.27.0",
    "class-validator": "*",
    "connect-redis": "*",
    "cors": "*",
    "dotenv": "*",
    "express": "^4.17.1",
    "express-session": "*",
    "graphql": "^15.4.0",
    "pg": "^8.6.0",
    "redis": "*",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.0",
    "typeorm": "^0.2.33"
  }
}
